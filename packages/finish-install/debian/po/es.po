#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:2001
msgid "Finishing the installation"
msgstr "Finalizando la instalación"

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:3001
msgid "Running ${SCRIPT}..."
msgstr "Ejecutando ${SCRIPT}..."

#. Type: text
